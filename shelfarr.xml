<?xml version="1.0"?>
<Container version="2">
  <Name>shelfarr</Name>
  <Repository>vacantlycrushing/shelfarr:latest</Repository>
  <Registry>https://hub.docker.com/r/vacantlycrushing/shelfarr</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
